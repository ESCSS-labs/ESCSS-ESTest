{
  "name": "escss-estest",
  "version": "2.6.2",
  "description": "Non-intrusive JavaScript runtime validator",
  "keywords": [
    "JavaScript",
    "zero-dependency",
    "runtime-testing",
    "schema-validation"
  ],
  "repository": "https://github.com/ESCSS-labs",
  "author": "Mike Lee",
  "license": "(AGPL-3.0-only OR Commercial)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    }
  },
  "main": "./src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "format": "prettier src test --write --cache",
    "lint": "eslint src test --fix --cache",
    "check": "pnpm format && pnpm lint && pnpm vitest",
    "format:check": "prettier --check .",
    "test": "clear && node ./src/index.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "vitest": "^4.0.15"
  }
}
